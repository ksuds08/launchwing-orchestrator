{
  "name": "launchwing-orchestrator",
  "private": true,
  "type": "module",
  "workspaces": ["app"],
  "scripts": {
    "build": "npm run build -w app && npm run postbuild",
    "postbuild": "rm -rf public && mkdir -p public && cp -r app/dist/* public/",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . || true"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20240725.0",
    "typescript": "5.5.4",
    "eslint": "9.9.0"
  },
  "engines": {
    "node": ">=20"
  }
}