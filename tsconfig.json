{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "lib": ["ES2021", "WebWorker"],
    "moduleResolution": "Bundler",
    "strict": true,
    "types": ["@cloudflare/workers-types"],
    "baseUrl": "./src",
    "paths": {
      "@utils/*": ["utils/*"],
      "@api/*": ["api/*"]
    }
  },
  "include": ["src/**/*"]
}